{
    "$schema": "https://raw.githubusercontent.com/dotnet/Nerdbank.GitVersioning/main/src/NerdBank.GitVersioning/version.schema.json",
    "version": "0.1-alpha",
    "assemblyVersion": {
      "precision": "revision"
    },
    "publicReleaseRefSpec": [
      "^refs/heads/main$",
      "^refs/heads/develop$",
      "^refs/heads/v\\d+\\.\\d+$"
    ],
    "cloudBuild": {
      "setVersionVariables": false,
      "buildNumber": {
        "enabled": false
      }
    }
  }