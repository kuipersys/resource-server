{
  "$schema": "https://raw.githubusercontent.com/dotnet/Nerdbank.GitVersioning/main/src/NerdBank.GitVersioning/version.schema.json",
  "version": "0.1-alpha",
  "assemblyVersion": {
    "precision": "revision"
  },
  "nuGetPackageVersion": {
    "semVer": 2.0
  },
  "publicReleaseRefSpec": [
    "^refs/heads/release$",
    "^refs/heads/release/v\\d+\\.\\d+$",
    "^refs/heads/v\\d+\\.\\d+$"
  ],
  "release": {
    "branchName": "release/v{version}"
  }
}